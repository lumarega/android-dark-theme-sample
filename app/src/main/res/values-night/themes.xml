<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- These are the values that will be picked up when we change to dark theme -->
    <style name="BlueTheme" parent="BaseTheme">
        <item name="colorPrimary">@color/primaryBrandColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkBrandColor</item>
        <item name="colorAccent">@color/secondaryBrandColor</item>
    </style>

    <style name="OrangeTheme" parent="BaseTheme">
        <item name="fontFamily">@font/oswald</item>
        <item name="colorPrimary">@color/primaryBrand2Color</item>
        <item name="colorPrimaryDark">@color/primaryDarkBrand2Color</item>
        <item name="colorAccent">@color/secondaryBrand2Color</item>
    </style>
</resources>